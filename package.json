{
  "name": "komondor-api",
  "version": "1.0.0",
  "description": "TSL Sequence Data API",
  "author": "Martin Page",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node ./src/server.js",
    "server:prod": "node ./dist/server.js",
    "dev": "NODE_ENV=development npm-run-all server",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "pm2": "pm2 start ./dist/server.js --name komondor-api",
    "watch": "nodemon",
    "test": "jest"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "ldapauth-fork": "^4.2.0",
    "moment": "^2.24.0",
    "mongoose": "^5.7.6",
    "npm-run-all": "^4.1.5",
    "tus-node-server": "^0.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "jest": "^24.9.0",
    "supertest": "^4.0.2"
  }
}
